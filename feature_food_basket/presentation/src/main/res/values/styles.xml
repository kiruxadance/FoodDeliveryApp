<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ThemeOverlay.App.Button" parent="">
        <item name="colorPrimary">@color/button_background</item>
        <item name="colorOnPrimary">@color/button_background</item>
    </style>

    <style name="basket_count">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="list_title">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="list_description">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="text_city">
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="text_time">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">#80000000</item>
    </style>
    <style name="roundedImageViewRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0%</item>
    </style>
    <style name="text_categories">
        <item name="android:textSize">20sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Button.Blue" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@color/button_background</item>
    </style>

</resources>